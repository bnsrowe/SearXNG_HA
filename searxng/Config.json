{
  "name": "Private SearXNG",
  "version": "1.0.0",
  "slug": "searxng",
  "description": "Private, self-hosted SearXNG instance running inside Home Assistant OS.",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8080]",
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "SearXNG Web Interface"
  },
  "map": [
    "config:rw"
  ],
  "schema": {},
  "host_network": false,
  "options": {},
  "arch": ["aarch64", "amd64"],
  "environment": {
    "SEARXNG_SETTINGS_PATH": "/config/searxng/settings.yml",
    "SEARXNG_SECRET_KEY": "CHANGE_THIS"
  }
}
